PYTHON

import requests
import json
url = "https://ap-south-1.aws.data.mongodb-api.com/app/data-nlwmghl/endpoint/data/v1/action/findOne"

payload = json.dumps({
    "collection": "<COLLECTION_NAME>",
    "database": "FlowterDB",
    "dataSource": "Flowter-Cluster",
    "projection": {
        "_id": 1
    }
})
headers = {
  'Content-Type': 'application/json',
  'Access-Control-Request-Headers': '*',
  'api-key': 'GyI6vNMdJNHVvfXQtJ9oWv8Gas1nPVKrPp0LoPeCSHA5BIfFEBDJMbiI7lwAhlor',
}

response = requests.request("POST", url, headers=headers, data=payload)

print(response.text)





import requests
import json
url = "https://ap-south-1.aws.data.mongodb-api.com/app/data-nlwmghl/endpoint/data/v1/action/findOne"

payload = json.dumps({
    "collection": "<COLLECTION_NAME>",
    "database": "FlowterDB",
    "dataSource": "Flowter-Cluster",
    "projection": {
        "_id": 1
    }
})
headers = {
  'Content-Type': 'application/json',
  'Access-Control-Request-Headers': '*',
  'api-key': 'GyI6vNMdJNHVvfXQtJ9oWv8Gas1nPVKrPp0LoPeCSHA5BIfFEBDJMbiI7lwAhlor',
  'Accept': 'application/ejson'
}

response = requests.request("POST", url, headers=headers, data=payload)

print(response.text)




